{
    "openapi": "3.0.0",
    "info": {
        "title": "Schedule API",
        "description": "API для работы с функционалом раписаний занятий в Университетах",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/admin/schedule_settings": {
            "get": {
                "tags": [
                    "Schedule settings"
                ],
                "summary": "Получение списка режимов звонков",
                "operationId": "get_schedule_settings",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Schedule settings"
                ],
                "summary": "Создание режима звонков",
                "operationId": "add_schedule_setting",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "count",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/api/v1/admin/schedule_settings/{scheduleSettingId}": {
            "get": {
                "tags": [
                    "Schedule settings"
                ],
                "summary": "Получение одного режима звонков",
                "operationId": "show_schedule_setting",
                "parameters": [
                    {
                        "name": "scheduleSettingId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "Schedule settings"
                ],
                "summary": "Обновление режима звонков",
                "operationId": "edit_schedule_setting",
                "parameters": [
                    {
                        "name": "scheduleSettingId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "schedule_setting_items": {
                                        "type": "array",
                                        "items": {
                                            "properties": {
                                                "time_start": {
                                                    "type": "string"
                                                },
                                                "time_end": {
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Schedule settings"
                ],
                "summary": "Удаление режима звонков",
                "operationId": "delete_schedule_setting",
                "parameters": [
                    {
                        "name": "scheduleSettingId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "token": {
                "type": "apiKey",
                "description": "Enter token in string format!",
                "name": "token",
                "in": "header"
            },
            "x_account_id": {
                "type": "apiKey",
                "description": "iDesk account ID",
                "name": "X-Account-Id",
                "in": "header"
            }
        }
    },
    "security": [
        {
            "token": [
                "get_settings",
                "add_setting",
                "edit_setting",
                "show_setting",
                "delete_setting"
            ],
            "x_account_id": [
                "get_settings",
                "add_setting",
                "edit_setting",
                "show_setting",
                "delete_setting"
            ]
        }
    ]
}